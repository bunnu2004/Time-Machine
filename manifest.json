{
    "manifest_version": 3,
    "name": "Tab Time Machine",
    "version": "1.0",
    "description": "Save, organize, and restore your browser tabs with ease.",
    "permissions": ["storage", "tabs", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  